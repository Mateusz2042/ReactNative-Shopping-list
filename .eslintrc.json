{
    "env": {
        "node": true,
        "browser": true,
        "es6": true,
        "amd": true
    },
    "parser": "babel-eslint",
    "plugins": [
        "react"
    ],
    "extends": "airbnb",
    "rules": {
        "react/jsx-filename-extension": [ 1, { "extensions": [ ".js", ".jsx" ] } ],
        "no-debugger": 0,
        "class-methods-use-this": 0,
        "camelcase": "off",
        "linebreak-style": 0,
        "no-console": 0,
        "import/prefer-default-export": 0,
        "react/destructuring-assignment": 0
    }
}
